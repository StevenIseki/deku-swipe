{
  "name": "deku-swipe",
  "version": "0.0.1",
  "description": "Swipe.js as a deku component",
  "main": "lib/swipe.js",
  "jsnext:main": "src/swipe.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/deku-swipe.git"
  },
